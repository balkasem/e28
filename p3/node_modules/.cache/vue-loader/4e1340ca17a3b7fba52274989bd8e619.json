ort default {\n  data() {\n    return {\n      post: {\n        title: \"New Article\",\n        content: \"content of my new post\",\n        auther: \"Sam Balkasem\",\n        favorite: true\n      }\n    };\n  },\n  methods: {\n    addPost() {\n      axios.post(\"/post\", this.post).then(response => {\n        console.log(response.data);\n      });\n    }\n  }\n};\n</script>\n"]}]}